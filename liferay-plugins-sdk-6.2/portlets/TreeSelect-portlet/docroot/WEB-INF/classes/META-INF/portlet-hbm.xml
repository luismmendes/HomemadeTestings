<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="home.testing.model.ProfessionalCodes" />
	<class name="home.testing.model.impl.ProfessionalCodesImpl" table="IEFP_ProfessionalCodes">
		<cache usage="read-write" />
		<id name="professionalCodesId" type="int">
			<generator class="increment" />
		</id>
		<property name="level" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="code" type="com.liferay.portal.dao.orm.hibernate.StringType" column="code_" />
		<property name="designation" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>